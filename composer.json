{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "darryldecode/cart": "^4.1",
        "davmixcool/laravel-meta-manager": "^1.0",
        "dimsav/laravel-translatable": "^9.4",
        "fideloper/proxy": "^4.0",
        "funkjedi/composer-include-files": "^1.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.1",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "leantony/laravel-grid": "^2.0",
        "nwidart/laravel-modules": "^5.0",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/message": "^1.8",
        "pimax/fb-messenger-php": "^2.0",
        "predis/predis": "^1.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "shvetsgroup/laravel-email-database-log": "^1.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.36",
        "sudiptpa/ipstack": "^1.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "include_files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
